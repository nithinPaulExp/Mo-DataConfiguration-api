    "@optimuz/cors": "0.0.3",
    "@optimuz/mysql": "0.0.3",
    "@optimuz/ping": "0.0.1",
    "@optimuz/redis": "0.0.2",
    "@optimuz/sns": "0.0.12",
    "@optimuz/user-service": "0.0.87",
    "aws-serverless-express": "3.3.6",
